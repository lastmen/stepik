def is_palindrome(text):
    count = 0
    b = []
    for i in range(len(text)):
        if text[i].isalpha() == True:
            b.append(text[i].lower())
    b = "".join(b)
    for i in range(1, len(b) + 1):
        if b[i - 1] == b[-i]:
            count += 1
        else: break
    if count == len(b):
        return True
    else:
        return False



a = input()
print(is_palindrome(a))
